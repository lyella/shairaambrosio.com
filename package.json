    "gray-matter": "^4.0.3",
    "invariant": "^2.2.4",
    "lib": "^5.1.0",
    "lottie-react": "^2.4.0",
    "next": "15.0.3",
    "next-mdx-remote": "^5.0.0",
    "next-pwa": "^5.6.0",
    "next-transpile-modules": "^10.0.1",
    "plaiceholder": "^3.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dom-confetti": "^0.2.0",
    "react-github-btn": "^1.4.0",
