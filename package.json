    "@svgr/webpack": "^8.1.0",
    "@types/gtag.js": "^0.0.20",
    "@types/invariant": "^2.2.37",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-instantsearch-dom": "^6.12.8",
    "@types/react-lottie": "^1.2.10",
